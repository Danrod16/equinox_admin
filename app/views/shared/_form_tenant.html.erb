<%= simple_form_for tenant, html: { class: class_list } do |f| %>
  <div class="form-group-three">
    <%= f.input :first_name, class: "urbanist-form-field" %>
    <%= f.input :last_name, class: "urbanist-form-field" %>
    <%= f.input :photo, class: "urbanist-form-field" %>
  </div>
  <div class="form-group-three">
    <%= f.input :email, class: "urbanist-form-field" %>
    <%= f.input :phone, class: "urbanist-form-field" %>
    <%= f.input :nationality, class: "urbanist-form-field" %>
  </div>
  <div class="form-group-three">
    <%= f.input :street, class: "urbanist-form-field" %>
    <%= f.input :street_2, class: "urbanist-form-field" %>
    <%= f.input :number, class: "urbanist-form-field" %>
  </div>

  <div class="form-group-three">
    <%= f.input :postal_code, class: "urbanist-form-field" %>
    <%= f.input :city, class: "urbanist-form-field" %>
    <%= f.input :country, as: :select, collection: ApplicationRecord::COUNTRIES, class: "urbanist-form-field" %>
  </div>
  <div class="form-group-three">
    <%= f.input :id_number, class: "urbanist-form-field" %>
    <%= f.input :id_type, class: "urbanist-form-field" %>
    <%= f.input :cif, class: "urbanist-form-field" %>
  </div>
  <%= f.input :description, class: "urbanist-form-field" %>
  <div class="form-actions">
    <%= f.button :submit, "Submit",  class: "button-primary" %>
  </div>
<% end %>
