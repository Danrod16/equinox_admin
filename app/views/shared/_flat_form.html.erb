<%= simple_form_for flat, html: { class: class_list } do |f| %>
  <%= f.input :celula %>
  <%= f.input :catastral %>
  <%= f.input :index %>
  <%= f.input :cee %>
  <%= f.input :expenses %>
  <%= f.input :insurance %>
  <%= f.input :insurance_phone %>
  <%= f.input :keys %>
  <%= f.input :equinox_costs %>
  <%= f.input :photos %>
  <%= f.input :video %>
  <%= f.input :furnished %>
  <%= f.select :landlord_id, Landlord.all.pluck(:first_name, :id), {}, { class: "form-control selectize", prompt: "Select Landlord" } %>
  <%= f.input :available %>
  <%= f.input :name %>
  <%= f.input :reference %>
  <%= f.input :street %>
  <%= f.input :number %>
  <%= f.input :postal_code %>
  <%= f.input :city %>
  <%#= f.select :country %>
  <%= f.input :letter_box %>
  <%= f.input :reference_rent %>
  <%= f.input :habitability_number %>
  <%= f.input :catastral_number %>
  <%= f.button :submit, "Submit",  class: "button-primary" %>
<% end %>
