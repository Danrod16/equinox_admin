<%= simple_form_for tenant, html: { class: class_list } do |f| %>
  <%= f.input :first_name, class: "form-control" %>
  <%= f.input :last_name, class: "form-control" %>
  <%= f.input :email, class: "form-control" %>
  <%= f.input :phone, class: "form-control" %>
  <%= f.input :description, class: "form-control" %>
  <%= f.input :photo, class: "form-control" %>
  <%= f.input :street, class: "form-control" %>
  <%= f.input :street_2, class: "form-control" %>
  <%= f.input :number, class: "form-control" %>
  <%= f.input :postal_code, class: "form-control" %>
  <%= f.input :city, class: "form-control" %>
  <%= f.select :country, class: "form-control" %>
  <%= f.input :id_number, class: "form-control" %>
  <%= f.input :id_type, class: "form-control" %>
  <%= f.input :cif, class: "form-control" %>
  <%= f.input :nationality, class: "form-control" %>
  <%= f.button :submit, "Submit",  class: "button-primary" %>
<% end %>
